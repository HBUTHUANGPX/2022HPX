<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find simplebb)/param/DWA_TEB/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find simplebb)/param/DWA_TEB/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find simplebb)/param/DWA_TEB/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find simplebb)/param/DWA_TEB/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find simplebb)/param/DWA_TEB/base_local_planner_holonomic_params.yaml" command="load" />
        <rosparam file="$(find simplebb)/param/DWA_TEB/move_base_params.yaml" command="load" />
    </node>
</launch>
